		"children": [
			{
				"th.interactive": {
					"colspan": "3",
					"children": [
						{
							"span": {
								"data-valueof": "name",
								"content": "{{unitName}}"
							}
						},
						{
							"createUnitNameEditorNode()": [
								"{{unitIndex}}"
							]
						},
						{
							"span": {
								"data-valueof": "count",
								"content": "{{unitCount}}"
							}
						},
						{
							"createUnitCountEditorNode()": [
								"{{unitIndex}}"
							]
						}
					]
				}
			}
		]
	}
}